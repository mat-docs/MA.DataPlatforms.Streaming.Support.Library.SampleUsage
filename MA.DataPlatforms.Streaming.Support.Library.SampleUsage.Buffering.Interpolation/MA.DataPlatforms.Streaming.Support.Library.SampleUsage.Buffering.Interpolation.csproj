  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MA.DataPlatforms.Streaming.Support.Lib.Core" Version="1.3.3.83-dev" />
    <PackageReference Include="MESL.SQLRace.API" Version="2.1.25099.2" />
  </ItemGroup>

</Project>
